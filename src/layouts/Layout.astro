---
import Header from '../components/Header.astro';
import NavBar from '../components/NavBar.astro';
const { title } = Astro.props;
---

<!doctype html>
<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>{title}</title>
  </head>
  <header>
    <NavBar />
  </header>
  <main>
    <body class=''>
      <slot />
    </body>
  </main>
</html>
